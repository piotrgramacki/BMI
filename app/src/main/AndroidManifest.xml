<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.piotrek.bmi238493">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.example.piotrek.bmi238493.MainActivity"
            android:launchMode="singleTop">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.piotrek.bmi238493.ResultActivity"
            android:parentActivityName="com.example.piotrek.bmi238493.MainActivity" />
        <activity
            android:name="com.example.piotrek.bmi238493.AboutActivity"
            android:theme="@style/AppTheme.NoActionBar"/>
    </application>

</manifest>